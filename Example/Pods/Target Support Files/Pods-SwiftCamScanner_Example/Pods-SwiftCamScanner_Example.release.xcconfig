ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
CLANG_WARN_DOCUMENTATION_COMMENTS = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/OpenCV" "$PODS_CONFIGURATION_BUILD_DIR/SwiftCamScanner"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/OpenCV/opencv2.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SwiftCamScanner/SwiftCamScanner.framework/Headers"
OTHER_CPLUSPLUSFLAGS = $(inherited) -fvisibility=hidden
OTHER_LDFLAGS = $(inherited) -ObjC -framework "SwiftCamScanner" -framework "opencv2"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
